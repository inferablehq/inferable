{
  "name": "@inferable/cli",
  "version": "0.24.5",
  "description": "CLI for inferable.ai",
  "bin": {
    "inf": "bin/index.js"
  },
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "test:dev": "jest --runInBand --watch src --setupFiles dotenv/config --forceExit --onlyChanged",
    "test": "jest --runInBand --setupFiles dotenv/config --forceExit",
    "start": "node ./bin/index.js",
    "build": "tsc && chmod +x ./bin/index.js",
    "clean": "rm -rf ./bin",
    "dev": "npm run build && npm run start"
  },
  "author": "Inferable, Inc.",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@graphql-tools/graphql-file-loader": "^8.0.1",
    "@graphql-tools/load": "^8.0.2",
    "@inquirer/prompts": "^4.0.0",
    "@openapi-contrib/openapi-schema-to-json-schema": "^5.1.0",
    "@ts-rest/core": "^3.28.0",
    "@types/blessed": "^0.1.25",
    "@types/unzipper": "^0.10.9",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.5",
    "chalk": "^4.1.2",
    "cli-spinner": "^0.2.10",
    "dotenv": "^16.4.5",
    "execa": "^5.1.1",
    "graphql": "^16.9.0",
    "handlebars": "^4.7.8",
    "inquirer": "^12.0.0",
    "json-colorizer": "^3.0.1",
    "json-schema": "^0.4.0",
    "json-schema-to-zod": "^2.4.0",
    "jsonwebtoken": "^9.0.2",
    "simple-update-notifier": "^2.0.0",
    "typescript": "^5.2.2",
    "unzipper": "^0.12.2",
    "yargs": "^17.7.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/cli-spinner": "^0.2.3",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/update-notifier": "^6.0.8",
    "@types/yargs": "^17.0.32",
    "axios-mock-adapter": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.7.0",
    "openapi-types": "^12.1.3"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ]
  }
}
