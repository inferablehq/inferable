{
  "name": "@inferable/react",
  "version": "0.0.17",
  "description": "React SDK for inferable.ai",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check $(git diff --diff-filter=d --name-only origin/main HEAD | grep -E \"\\.(ts|tsx)$\" || echo \".\") || exit 0",
    "prepublishOnly": "npm run clean && npm run format:check && npm run test && npm run build"
  },
  "author": "Inferable, Inc.",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "vite": "^5.0.2"
  },
  "dependencies": {
    "@ts-rest/core": "^3.28.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  }
}
