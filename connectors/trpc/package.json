{
  "name": "@inferable/trpc-adapter",
  "version": "0.1.0",
  "description": "Build your Inferable agent functions from your existing tRPC procedures.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx -r dotenv/config src/test.ts --watch src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "tsx -r dotenv/config src/test.ts",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@trpc/server": "^10.0.0",
    "inferable": "^0.30.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@trpc/server": "^10.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}