{
  "name": "inferable",
  "private": true,
  "version": "0.0.0",
  "description": "Inferable monorepo",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml}\""
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0"
  }
}
